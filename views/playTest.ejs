<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="playTest.css">
    <link rel="stylesheet" href="home.css">
    <title>Document</title>
</head>
<body class="bp-gradient">
    <nav class="navbar navbar-expand-lg navbar-dark bp-gradient text-light">
        <div class="container-fluid">
          <a class="navbar-brand" href="#">JeopLit</a>
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse justify-content-end" id="navbarNavAltMarkup">
            <div class="navbar-nav">
              <a class="nav-link active" aria-current="page">Home</a>
              <a class="nav-link" href="aboutPage">About</a>
              <a class="nav-link" href="contactPage">Contact</a>
              <% if (locals.user) { %>
                <a class="nav-link">Welcome, <%= user.username %>!</a>
                <a class="nav-link" href="logout">Logout</a>
              <% } else { %>
                <a class="nav-link" href="loginPage">Login</a>
                <a class="nav-link" href="signup">Sign up</a>
              <% } %>
              
              <!-- <a class="nav-link disabled" href="#" tabindex="-1" aria-disabled="true">Disabled</a> -->
            </div>
          </div>
        </div>
      </nav>
    <div class="categories bp-gradient p-2">
        <h1 class="text-center text-white">Study Smart with JeoPlit</h1>
        <div class="d-flex text-white justify-content-evenly">
          <h4>Category 1</h4>
          <h4>Category 2</h4>
          <h4>Category 3</h4>
          <h4>Category 4</h4>
          <h4>Category 5</h4>
        </div>
    </div>
    <div class="gameset d-flex container flex-row flex-wrap justify-content-start bp-gradient p-2 mr-45px">
        <div class="d-flex w-100 gap-2">
          <%for (q of questions.q1){%>
            <div class="w-20 question-box"><h3 class="question-text">100</h3></div>
          <%}%>
        </div>
        <div class="d-flex w-100 gap-2">
          <%for (q of questions.q2){%>
            <div class="w-20 question-box"><h3 class="question-text">200</h3></div>
          <%}%>
        </div>
        <div class="d-flex w-100 gap-2">
          <%for (q of questions.q3){%>
            <div class="w-20 question-box"><h3 class="question-text">300</h3></div>
          <%}%>
        </div>
        <div class="d-flex w-100 gap-2">
          <%for (q of questions.q4){%>
            <div class="w-20 question-box"><h3 class="question-text">400</h3></div>
          <%}%>
        </div>
        <div class="d-flex w-100 gap-2">
          <%for (q of questions.q5){%>
            <div class="w-20 question-box"><h3 class="question-text">500</h3></div>
          <%}%>
        </div>
    </div>
    <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModal">
      Launch demo modal
    </button>
    <!-- Modal -->
    <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div class="modal-body">
            <%let i = 0%>
            <%= answers[i]%>
            <% i++ %>
          </div>
        </div>
      </div>
    </div>
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
</body>
</html>